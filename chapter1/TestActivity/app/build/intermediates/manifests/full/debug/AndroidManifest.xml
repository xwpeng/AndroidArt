<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.xwpeng.testactivity"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="26" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >

        <!-- android:excludeFromRecents="true" -->
        <activity
            android:name="android.xwpeng.testactivity.MainActivity"
            android:label="TestActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="android.xwpeng.testactivity.life.AaActivity"
            android:configChanges="orientation|screenSize"
            android:label="AaActivity" />
        <activity
            android:name="android.xwpeng.testactivity.life.BbActivity"
            android:label="BbActivity"
            android:theme="@style/BbActivityTheme" />
        <activity
            android:name="android.xwpeng.testactivity.life.CcActivity"
            android:label="CcActivity" />
        <activity
            android:name="android.xwpeng.testactivity.startmode.StandardActivity"
            android:label="StartActivity" />
        <activity
            android:name="android.xwpeng.testactivity.startmode.SingleTopActivity"
            android:label="SingleTopActivity"
            android:launchMode="singleTop" />
        <activity
            android:name="android.xwpeng.testactivity.startmode.SingleTaskActivity"
            android:label="SingleTaskActivity"
            android:launchMode="singleTask"
            android:taskAffinity="xwpeng.com" />
        <activity
            android:name="android.xwpeng.testactivity.startmode.SingleInstanceActivity"
            android:label="SingleInstanceActivity"
            android:launchMode="singleInstance" />
        <activity
            android:name="android.xwpeng.testactivity.intentfilter.AimActivity"
            android:label="AimActivity" >
            <intent-filter>
                <action android:name="com.xwpeng.action2" />

                <category android:name="android.intent.category.DEFAULT" />
                <!-- <data android:mimeType="text/plain"/> -->
            </intent-filter>
            <intent-filter>
                <action android:name="com.xwpeng.action1" />

                <!-- <category android:name="android.intent.category.BROWSABLE" /> -->
                <category android:name="android.intent.category.DEFAULT" />

                <!--
                  <data android:scheme="xwpeng" />
                  <data android:scheme="xwpeng2" />
                -->
                <!-- <data android:mimeType="image/png" android:scheme="file"/> -->
                <data android:mimeType="image/png" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="android.support.VERSION"
            android:value="26.1.0" />
        <meta-data
            android:name="android.arch.lifecycle.VERSION"
            android:value="27.0.0-SNAPSHOT" />
    </application>

</manifest>